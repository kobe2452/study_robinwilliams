\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 Local groups seek to raise suicide awareness\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 The Lebanon Democrat, Tenn.
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 STATE AND REGIONAL NEWS
\par \pard 
