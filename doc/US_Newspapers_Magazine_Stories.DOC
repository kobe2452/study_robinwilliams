\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SUICIDE CASES HARD TO PROVE\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 RUSSELL ROBINSON
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 NEWS; Pg. 2
