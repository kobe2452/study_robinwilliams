\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 NEW DRIVE TO CUT SUICIDE TRAGEDIES; 
\par \pard \ql \li0\ri0 \sb0 \fi0 Buddies join campaign \fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 KATIE KELLY
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 NEWS; Pg. 18
\par \pard 
