\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 One tough topic\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 Canadian Champion editorial
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 EDITORIAL; Pg. 1
\par \pard 
