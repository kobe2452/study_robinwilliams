\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 By Amy Faulstich Guest op-ed column
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 OPINION
\par \pard 
