\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 Theresa King, The Edmonton Journal
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 LETTERS; Pg. A17
\par \pard 
